<style>
    .div1 {
        width: 600px;
        height: 120px;
        border: 3px solid cornflowerblue;
    }

    ul {
        list-style-type: none;
    }

    body {

        background-color: white;
    }
    @font-face {
   font-family: myFirstFont;
   src: url(sansation_light.woff);
}

div {
   font-family: myFirstFont;
}



