<!-- {{#each products}}
    <h1><a href="{{this.link}}">{{this.source}}</a></h1> -->
<header class="w3-container w3-blue">
    
        <h1 style="font-family:Sansation Light">
           <center>Job Board- By Go Resume</center>
        </h1>
        <h3 style="font-family:Sansation Light">
            <center>Bringing you the latest jobs that you might find suitable</center>
        </h3>
    </header>

    <a class="btn btn-success navbar-btn" style="font-family:Sansation Light"
                            role="button" href="https://harissiddiqui.github.io/goresume/" target="_blank" />Home</a>
           <a class="btn btn-primary" style="font-family:Sansation Light"
                            role="button" href="https://harissiddiqui.github.io/goresume/" target="_blank" />Create Resume</a>
<body background="img.jpg">
    <br/>
    <br/>
    <ul>
        {{#each this.items}}
        <div class="row">
            <div class="col-sm-3"></div>

            <div class="col-sm-4">
                <div class="div1">
                    <li style="font-size: 20px"><a target="_blank" href="https://www.indeed.com{{this.link}}"> <strong>     {{this.title}}    </strong></a>
                        <li style="font-size: 18px"><a target="_blank" href="https://www.indeed.com{{this.link}}"><strong>At         </strong>    {{this.company}}  </a></li>
                        <li style="font-size: 14px"><strong>In         </strong> {{this.location}}</li>
                        <li style="font-size: 14px"><strong>Dated      </strong> {{this.date}} </</li>
                            {{!--
                            <li><a target="_blank" href="https://www.indeed.com{{this.link}}"><strong>At         </strong>    {{this.company}}  </a></li>
                            <li><a target="_blank" href="https://www.indeed.com{{this.link}}"><strong>In         </strong>    {{this.location}} </a></li>
                            <li><a target="_blank" href="https://www.indeed.com{{this.link}}"><strong>Dated      </strong>    {{this.date}}     </a></li>
                            --}}
                </div>
            </div>
        </div>

        <br> {{/each}}

    </ul>

    {{/each}}
